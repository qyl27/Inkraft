{
  "schema_version": 1,
  "mixin": [
    "inkraft.mixins.json",
    "inkraft-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "inkraft",
    "version": "${version}",
    "metadata": {
      "name": "Inkraft",
      "description": "Use ink language to write your story in Minecraft.",
      "contributors": {
        "qyl27": "Owner"
      },
      "contact": {
        "sources": "https://github.com/qyl27/inkraft"
      },
      "icon": "assets/inkraft/icon.png",
      "license": "Apache-2.0"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "cx.rain.mc.inkraft.quilt.InkraftQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.0.8"
      }
    ]
  }
}